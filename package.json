{
  "name": "nextjs-fastapi",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "fastapi-dev": "pip3 install -r requirements.txt && python3 -m uvicorn api.index:app --reload",
    "next-dev": "next dev",
    "dev": "npm run next-dev",
    "build": "npm run build:textbook && next build",
    "build:textbook": "pnpm --filter {textbook} run build && mkdir -p public/docs && cp -r textbook/build/* public/docs/",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@ai-sdk/react": "^2.0.76",
    "@radix-ui/react-slot": "^1.1.0",
    "@vercel/analytics": "^1.3.1",
    "@vercel/kv": "^2.0.0",
    "ai": "^5.0.76",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "concurrently": "^8.0.1",
    "date-fns": "^4.1.0",
    "eslint-config-next": "13.4.4",
    "framer-motion": "^11.11.17",
    "geist": "^1.3.1",
    "lucide-react": "^0.460.0",
    "next": "13.4.4",
    "prism-react-renderer": "^2.4.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-markdown": "^9.0.1",
    "react-refresh": "^0.18.0",
    "remark-gfm": "^4.0.0",
    "sonner": "^1.5.0",
    "tailwind-merge": "^2.5.4",
    "usehooks-ts": "^3.1.0"
  },
  "devDependencies": {
    "@types/node": "20.2.4",
    "@types/react": "18.2.7",
    "@types/react-dom": "18.2.4",
    "autoprefixer": "10.4.14",
    "eslint": "8.41.0",
    "postcss": "8.4.23",
    "tailwindcss": "3.4.15",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "5.0.4"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "18.2.7",
      "@types/react-dom": "18.2.4"
    }
  }
}